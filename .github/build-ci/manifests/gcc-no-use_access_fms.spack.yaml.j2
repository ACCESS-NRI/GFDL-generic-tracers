spack:
  specs:
  # *_compiler is defined in the .github/build-ci/data/standard.json file
  - access-generic-tracers@git.{{ ref }} ~use_access_fms
  packages:
    fms:
      require:
        - '@git.2025.02=2025.02 ~openmp precision=64'
    all:
      require:
      - '%{{ gcc_compiler }} target=x86_64'
  concretizer:
    unify: false
  view: false
